@using System.Collections.ObjectModel
@{
    Layout = "_Layout";
    if (!ViewData.ContainsKey("Sections"))
    {  
        ViewData["Sections"] = new Collection<KeyValuePair<string, string>>()
        {
            KeyValuePair.Create(Url.Action("Index", "Home") !, "Home"),
            KeyValuePair.Create(Url.Action("Index", "Home", new { area = "IngredientsSelection" }) !, "Kill your leftovers 🔪"),
            KeyValuePair.Create(Url.Action("Index", "Home", new { area = "BackOffice" }) !, "Back office 💀")
        };
    }
}
